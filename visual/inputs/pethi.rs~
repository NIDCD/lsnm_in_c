%  WM Task with 1-second delay

WAPet 10		% Write PET every 10 iterations and reset

Randseed  0	%seed for random number generator

Run 100		% 100 ticks without input

% *************************************************************
%  STIMULUS # 1
% *********************


#include /home/horwitz/barrysim/sfiles/hiattn_r.s	% High Attention
#include /home/horwitz/barrysim/inputs/input1.inp 	 %  Input ON
Run 200					 %  200=1sec.
#include /home/horwitz/barrysim/inputs/noinp.lgn  	 %  Input OFF
Run 200					 %  Delay	
#include /home/horwitz/barrysim/inputs/input1.inp 	 %  Input ON-match
Run 200
#include /home/horwitz/barrysim/inputs/noinp.lgn  	 %  Input OFF
#include /home/horwitz/barrysim/sfiles/loattn_r.s

Run 100

% ********************   INTERTRIAL INTERVAL


#include /home/horwitz/barrysim/sfiles/reset_r.s    % Input OFF and reset delay units
Run 100


