cd /home/bhguest/rongf/auditory/sfiles
/home/bhguest/rongf/auditory/bin/mkattn_au hiattn_l.s 0.20
/home/bhguest/rongf/auditory/bin/mkattn_au loattn_l.s 0.10
/home/bhguest/rongf/auditory/bin/mkattn_au loattn_r.s 0.05
cd /home/bhguest/rongf/auditory
rm auseq1.s
cat weights/right/auright.s weights/left/auleft.sn code/apprl1 > auseq1.s
rm code/apprl1
rm code/batchrlc_au
#
#
cd /home/bhguest/rongf/auditory
mkdir noisy
cd /home/bhguest/rongf/auditory/code
/home/bhguest/rongf/auditory/bin/altgenw_au 100 100 100 100 100 100 100 100 100 100 100 100 100
/home/bhguest/rongf/auditory/bin/altgenw_L_au 100 100 100 100 100 100 100 100 100 100 100 100 100
echo Subject 1
#
cd /home/bhguest/rongf/auditory/sfiles
/home/bhguest/rongf/auditory/bin/mkattn_au hiattn_r.s 0.30
cd /home/bhguest/rongf/auditory/weights/right
foreach file (*.ws)
  /home/bhguest/rongf/auditory/bin/netgenC_au $file
  end
cd ../left
foreach file (*.ws)
  /home/bhguest/rongf/auditory/bin/netgenC_L_au $file
  end
echo attn level: 0.30
cd /home/bhguest/rongf/auditory/noisy
mkdir b30
#
cd /home/bhguest/rongf/auditory/weights/cross
/home/bhguest/rongf/auditory/bin/crosswt_au_i netgenC_au
cat aucrosslist.txt crossinhib.txt > aucrosslist1.txt
cat sh_cross crossinhibsh.txt > sh_cross1
rm aucrosslist.txt
rm sh_cross
mv aucrosslist1.txt aucrosslist.txt
mv sh_cross1 sh_cross
chmod u+x sh_cross
echo Building cross connections...
sh_cross
rm sh_cross
cd /home/bhguest/rongf/auditory
/home/bhguest/rongf/auditory/bin/au_sim1 auseq1
gzip *.out
cd noisy/b30
mkdir trial1
cd /home/bhguest/rongf/auditory
mv spec_pet.m noisy/b30/trial1/
mv *.out* noisy/b30/trial1/
cp /home/bhguest/rongf/auditory/weights/cross/aucrosslist.txt noisy/b30/trial1/aucrosslist1.txt
#
#
cd /home/bhguest/rongf/auditory/sfiles
/home/bhguest/rongf/auditory/bin/mkattn_au hiattn_r.s 0.05
cd /home/bhguest/rongf/auditory/weights/right
foreach file (*.ws)
  /home/bhguest/rongf/auditory/bin/netgenC_au $file
  end
cd ../left
foreach file (*.ws)
  /home/bhguest/rongf/auditory/bin/netgenC_L_au $file
  end
echo attn level: 0.05
