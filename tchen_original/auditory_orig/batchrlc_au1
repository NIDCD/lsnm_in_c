cat sh_cross crossinhibsh.txt > sh_cross1
rm aucrosslist.txt
rm sh_cross
mv aucrosslist1.txt aucrosslist.txt
mv sh_cross1 sh_cross
chmod u+x sh_cross
echo Building cross connections...
sh_cross
rm sh_cross
cd /home/bhguest/rongf/auditory
/home/bhguest/rongf/auditory/bin/au_sim1 auseq1
gzip *.out
cd noisy/b05
mkdir trial1
