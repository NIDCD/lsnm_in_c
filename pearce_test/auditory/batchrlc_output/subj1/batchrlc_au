cd /home/ulloa/lsnm/pearce_test/auditory/sfiles
/home/ulloa/lsnm/pearce_test/auditory/bin/mkattn_au hiattn_l.s 0.20
/home/ulloa/lsnm/pearce_test/auditory/bin/mkattn_au loattn_l.s 0.10
/home/ulloa/lsnm/pearce_test/auditory/bin/mkattn_au loattn_r.s 0.05
cd /home/ulloa/lsnm/pearce_test/auditory/
rm auseq1.s
cat $LSNM/weights/right/auright.s $LSNM/weights/left/auleft.sn $LSNM/code/apprl1 > auseq1.s
rm $LSNM/code/apprl1
rm auseq2.s
cat $LSNM/weights/right/auright.s $LSNM/weights/left/auleft.sn $LSNM/code/apprl2 > auseq2.s
rm $LSNM/code/apprl2
#
#
cd /home/ulloa/lsnm/pearce_test/auditory/
mkdir $LSNM/noisy
cd /home/ulloa/lsnm/pearce_test/auditory//code
/home/ulloa/lsnm/pearce_test/auditory/bin/altgenw_au 100 100 100 100 100 100 100 100 100 100 100 100 100
/home/ulloa/lsnm/pearce_test/auditory/bin/altgenw_L_au 100 100 100 100 100 100 100 100 100 100 100 100 100
echo Subject 1
#
cd /home/ulloa/lsnm/pearce_test/auditory/sfiles
/home/ulloa/lsnm/pearce_test/auditory/bin/mkattn_au hiattn_r.s 0.20
cd /home/ulloa/lsnm/pearce_test/auditory/weights/right
for file in `ls *.ws`
 do /home/ulloa/lsnm/pearce_test/auditory/bin/netgenC_au $file
  done
cd ../left
for file in `ls *.ws`
 do /home/ulloa/lsnm/pearce_test/auditory/bin/netgenC_L_au $file
  done
echo attn level: 0.20
cd /home/ulloa/lsnm/pearce_test/auditory/noisy
mkdir b20
cd /home/ulloa/lsnm/pearce_test/auditory//weights/cross
/home/ulloa/lsnm/pearce_test/auditory/bin/crosswt_au_i netgenC_au
cat $LSNM/aucrosslist.txt crossinhib.txt > $LSNM/aucrosslist1.txt
cat $LSNM/sh_cross crossinhibsh.txt > $LSNM/sh_cross1
rm $LSNM/aucrosslist.txt
rm $LSNM/sh_cross
mv $LSNM/aucrosslist1.txt $LSNM/aucrosslist.txt
mv $LSNM/sh_cross1 $LSNM/sh_cross
chmod u+x sh_cross
echo Building cross connections...
./sh_cross
rm sh_cross
cd /home/ulloa/lsnm/pearce_test/auditory/
/home/ulloa/lsnm/pearce_test/auditory/bin/au_sim1 auseq1
gzip *.out
cd noisy/b20
mkdir trial1
cd /home/ulloa/lsnm/pearce_test/auditory/
mv spec_pet.m noisy/b20/trial1/
mv *.out* noisy/b20/trial1/
cp /home/ulloa/lsnm/pearce_test/auditory/weights/cross/aucrosslist.txt noisy/b20/trial1/aucrosslist1.txt
#
cd /home/ulloa/lsnm/pearce_test/auditory//weights/cross
/home/ulloa/lsnm/pearce_test/auditory/bin/crosswt_au_i netgenC_au
cat $LSNM/aucrosslist.txt crossinhib.txt > $LSNM/aucrosslist1.txt
cat $LSNM/sh_cross crossinhibsh.txt > $LSNM/sh_cross1
rm $LSNM/aucrosslist.txt
rm $LSNM/sh_cross
mv $LSNM/aucrosslist1.txt $LSNM/aucrosslist.txt
mv $LSNM/sh_cross1 $LSNM/sh_cross
chmod u+x sh_cross
echo Building cross connections...
./sh_cross
rm sh_cross
